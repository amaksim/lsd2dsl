include(GetPrerequisites)
set(MINGW_ROOT /usr/i686-w64-mingw32/sys-root/mingw)
set(gp_tool objdump)
file(COPY "${MINGW_ROOT}/lib/qt5/plugins/platforms/qwindows.dll" DESTINATION ${CMAKE_INSTALL_PREFIX}/platforms)
file(COPY "@CMAKE_BINARY_DIR@/console/lsd2dsl.exe" DESTINATION ${CMAKE_INSTALL_PREFIX})
file(COPY "@CMAKE_BINARY_DIR@/gui/lsd2dsl-qtgui.exe" DESTINATION ${CMAKE_INSTALL_PREFIX})
get_prerequisites("${CMAKE_INSTALL_PREFIX}/lsd2dsl.exe" prereq 0 1 "" "${MINGW_ROOT}/bin")
get_prerequisites("${CMAKE_INSTALL_PREFIX}/lsd2dsl-qtgui.exe" prereq 0 1 "" "${MINGW_ROOT}/bin")
get_prerequisites("${MINGW_ROOT}/lib/qt5/plugins/platforms/qwindows.dll" prereq 0 1 "" "${MINGW_ROOT}/bin")
foreach(dll IN LISTS prereq)
    set(path "/usr/i686-w64-mingw32/sys-root/mingw/bin/${dll}")
    if(EXISTS ${path})
        file(COPY ${path} DESTINATION ${CMAKE_INSTALL_PREFIX})
    endif()
endforeach()
